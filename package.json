{
  "name": "lioslab",
  "version": "0.2.0",
  "description": "Internet Computer starter application",
  "type": "module",
  "keywords": [
    "Internet Computer",
    "JavaScript",
    "Canister"
  ],
  "scripts": {
    "dev": "vite dev -d",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "prebuild": "npm run generate",
    "start": "webpack serve --mode development --env development",
    "deploy:local": "dfx deploy --network=local",
    "deploy:ic": "dfx deploy --network=ic",
    "generate": "dfx generate dfinity_js_backend",
    "gen-deploy:local": "dfx generate dfinity_js_backend && dfx deploy --network=local"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^5.0.5",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.4",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.4.16",
    "azle": "^0.21.1",
    "big-number": "^2.0.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.0",
    "leaflet": "^1.9.4",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.0",
    "sass": "^1.69.5",
    "svelte": "^4.2.3",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^5.1.0",
    "ts-node": "^11.0.0-beta.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "uuid": "^9.0.0",
    "web-vitals": "^1.0.1"
  },
  "dependencies": {
    "@dfinity/agent": "^1.2.0",
    "@dfinity/auth-client": "^1.2.0",
    "@dfinity/candid": "^1.2.0",
    "@popperjs/core": "^2.10.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "big-number": "^2.0.0",
    "bootstrap": "^5.1.3"
  },
  "engines": {
    "node": "^12 || ^14 || ^16 || ^18 || ^20 || ^21"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ]
}
